type Client {
    name: String
    missions: [Mission]
}

type Mission {
    title: String
    client: Client
}

type Person {
    # Soatien first name
    firstName: String
    # Soatien last name
    lastName: String
    company: String
    email: String
    age: Int
    active: Boolean
    phone: String
    mission: Mission
}

type Query {
    findAllSoatien: [Person]!
    findAllMissions: [Mission]!
}